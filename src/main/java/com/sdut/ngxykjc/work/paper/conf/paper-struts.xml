<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="paper" extends="basePackage">
        <action name="paper_*" class="paperAction" method="{1}">
            <result name="success">/jsp/paper/listUI.jsp</result>
            <result name="user">/jsp/paper/paper.jsp</result>
            <result name="add">/jsp/paper/addUI.jsp</result>
            <result name="listquery">/jsp/QueryStatistics/paper.jsp</result>
        </action>
    </package>
</struts>